{
  "name": "docs",
  "version": "0.0.1",
  "private": true,
  "description": "GitBook-style manual documentation",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --poll",
    "start:ko": "docusaurus start --poll --port 3000",
    "start:en": "docusaurus start --locale en --poll --port 3001",
    "start:both": "concurrently -n ko,en -c blue,green \"npm run start:ko\" \"npm run start:en\"",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "3.4.0",
    "@docusaurus/preset-classic": "3.4.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/tsconfig": "3.4.0",
    "@docusaurus/types": "3.4.0",
    "concurrently": "^9.1.0",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [">0.5%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  },
  "engines": {
    "node": ">=18.0"
  }
}
